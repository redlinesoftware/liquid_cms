<div id="components">
  <%= form_tag cms_components_upload_path, :multipart => true do %>
    <%= file_field_tag :zip_file %>
    <%= submit_tag 'Upload' %> <em>.zip files only</em>
  <% end %>

  <% if @cms_components.empty? %>
    <p><%= t 'components.actions.index.none' %></p>
  <% else %>
    <%= list_files @cms_components %>
  <% end %>
</div>
